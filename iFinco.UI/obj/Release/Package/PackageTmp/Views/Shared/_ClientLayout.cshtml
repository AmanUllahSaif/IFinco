    <!-- Background Area Start -->
    <!-- Background Area End -->
    <!-- Question Ask Area Start -->
    <div class="question-ask-area">
        <button class="toggle"><img src="~/ClientAssets/img/icons/ques.png" alt=""></button>
        <div class="question-popoup">
            <h6>Need Any Help?</h6>
            <p>Currently we are offline, you can leave a message and we will get back soon</p>
            <div class="question-info">
                <div class="q-image">
                    <img src="~/ClientAssets/img/testi/2.png" width="45" height="45" alt="">
                </div>
                <div class="q-text">
                    <h5>Maheen Shahid</h5>
                    <span>Live Support</span>
                </div>
                <div class="q-btn">
                    @if (Request.Url.AbsolutePath.ToLower().Equals("/") || Request.Url.AbsolutePath.ToLower().Equals("/home/index"))
                    {
                        <a href="#contact" class="default-button">contact</a>
                    }
                    else
                    {
                        <a href="@Url.Action("Index","Home")#contact" class="default-button">contact</a>

                    }
                </div>
            </div>
        </div>
    </div>
    <!-- Question Ask Area End -->
    <!-- About Area Start -->
    <!--End of Client area-->
