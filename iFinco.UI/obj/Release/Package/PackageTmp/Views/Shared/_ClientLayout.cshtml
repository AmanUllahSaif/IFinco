                                    <li>
                                        <a href="@Url.Action("Register","Company")">Register Company</a>
                                    </li>
                                    <li><a href="@Url.Action("Login","Account")">Login</a></li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
