<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="../assets/img/apple-icon.png" />
    <link rel="icon" type="image/png" href="../assets/img/favicon.png" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Material Dashboard by Creative Tim</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />
    <!-- Bootstrap core CSS     -->

    <link href="@Url.Content("~/")PanalAssets/css/bootstrap.min.css" rel="stylesheet" />
    <!--  Material Dashboard CSS    -->
    <link href="@Url.Content("~/")PanalAssets/css/material-dashboard.css?v=1.2.0" rel="stylesheet" />
    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="@Url.Content("~/")PanalAssets/css/demo.css" rel="stylesheet" />
    <!--     Fonts and icons     -->
    <link href="~/PanalAssets/vendor/bootstrapswitch/css/bootstrap-switch.css" rel="stylesheet" />
    <link href="~/PanalAssets/vendor/bootstrapswitch/css/highlight.css" rel="stylesheet" /><link href="~/PanalAssets/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/PanalAssets/css/material-icons.min.css" rel="stylesheet" />
    <link href="~/PanalAssets/css/roboto.min.css" rel="stylesheet" />
    @*<link href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
        <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons' rel='stylesheet' type='text/css'>*@
</head>

<body>
    <div class="wrapper">
        <div class="sidebar" data-color="purple" data-image="@Url.Content("~/")PanalAssets/img/sidebar-1.jpg">
            <!--
                Tip 1: You can change the color of the sidebar using: data-color="purple | blue | green | orange | red"

                Tip 2: you can also add an image using data-image tag
            -->
            <div class="logo">
                <a href="http://www.vnextsolution.com" class="simple-text">
                    VnextSolution
                </a>
            </div>
            <div class="sidebar-wrapper">
                <ul class="nav">
                    <li id="Dashboard">
                        <a href="@Url.Action("Index", "Home", new { area="admin"})">
                            <i class="material-icons">dashboard</i>
                            <p>Dashboard</p>
                        </a>
                    </li>
                    <li id="Company">
                        <a href="@Url.Action("Index", "Company", new { area="admin"})">
                            <i class="material-icons">store</i>
                            <p>Company Managment</p>
                        </a>
                    </li>
                    <li id="Category" class="has-child">
                        <a href="javascript:void(0);">
                            <i class="material-icons">
                                settings applications
                            </i>
                            <p>Setup</p>
                        </a>
                        <ul class="nav child-nav">
                            <li>
                                <a href="#">
                                    <i class="material-icons">person_add</i>
                                    User Managment
                                </a>
                            </li>
                        </ul>
                    </li>

                    <li class="has-child">
                        <a href="javascript:void(0);">
                            <i class="material-icons">
                                settings applications
                            </i>
                            <p>Menu Management</p>
                        </a>
                        <ul class="nav child-nav">
                            <li id="Menu">
                                <a href="@Url.Action("Index", "Menu", new { area="admin"})">
                                    <i class="material-icons">store</i>
                                    Menu
                                </a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
        <div class="main-panel">
            <nav class="navbar navbar-transparent navbar-absolute">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#"> @ViewBag.Title </a>
                    </div>
                    <div class="collapse navbar-collapse">
                        @Html.Partial("~/Views/Shared/_PanelLoginPartial.cshtml")
                        <form class="navbar-form navbar-right" role="search">
                            <div class="form-group  is-empty">
                                <input type="text" class="form-control" placeholder="Search">
                                <span class="material-input"></span>
                            </div>
                            <button type="submit" class="btn btn-white btn-round btn-just-icon">
                                <i class="material-icons">search</i>
                                <div class="ripple-container"></div>
                            </button>
                        </form>
                    </div>
                </div>
            </nav>

            @RenderBody()
            <footer class="footer">
                <div class="container-fluid">
                    <nav class="pull-left">
                        <ul>
                            <li>
                                <a href="#">
                                    Home
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    Company
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    Portfolio
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    Blog
                                </a>
                            </li>
                        </ul>
                    </nav>
                    <p class="copyright pull-right">
                        &copy;
                        <script>
                            document.write(new Date().getFullYear())
                        </script>
                        <a href="http://www.vnestsolution.com">VnextSolution</a>, made with love for a better web
                    </p>
                </div>
            </footer>
        </div>
    </div>
    <!--   Core JS Files   -->
    <script src="@Url.Content("~/")PanalAssets/js/jquery-3.2.1.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/")PanalAssets/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/")PanalAssets/js/material.min.js" type="text/javascript"></script>

    <!--  Dynamic Elements plugin -->
    <script src="@Url.Content("~/")PanalAssets/js/arrive.min.js"></script>
    <!--  PerfectScrollbar Library -->
    <script src="@Url.Content("~/")PanalAssets/js/perfect-scrollbar.jquery.min.js"></script>
    <!--  Notifications Plugin    -->
    <script src="@Url.Content("~/")PanalAssets/js/bootstrap-notify.js"></script>
    <!--  Google Maps Plugin    -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE"></script>
    <script src="~/PanalAssets/vendor/bootstrapswitch/js/bootstrap-switch.js"></script>
    <script src="~/PanalAssets/vendor/bootstrapswitch/js/highlight.js"></script>
    <script src="@Url.Content("~/")PanalAssets/js/material-dashboard.js"></script>
    <script src="~/Scripts/common/Nav.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('.switch[type="checkbox"], .switch[type="radio"]')
     .not('[data-switch-no-init]')
     .bootstrapSwitch()
            //$.validator.methods.date = function (value, element) {
            //    return this.optional(element) || moment(value, "DD.MM.YYYY", true).isValid();
            //}
            // Javascript method's body can be found in assets/js/demos.js
            //demo.initDashboardPageCharts();
            $('[data-toggle="tooltip"]').tooltip();
        });
    </script>
    @RenderSection("Scripts", required: false)

</body>
</html>
